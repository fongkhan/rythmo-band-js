      <lipsync timecode="01:13:59:12" type="out_open"/>
    </line>
    <line role="mirai" track="1">
      <lipsync timecode="01:14:01:10" type="in_open"/>
      <text>Rien ...</text>
      <lipsync timecode="01:14:02:03" type="out_open"/>
    </line>
    <line role="akihito" track="0">
      <lipsync timecode="01:14:04:23" type="in_open"/>
